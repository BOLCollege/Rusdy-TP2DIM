{% extends 'base.html.twig' %}

{% block title %}Hello MainController!{% endblock %}

{% block body %}
<div class="container">
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Telephone</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    {% for data in list %}
    <tr>
        <th scope="row">{{data.id}}</th>
        <td>{{data.name}}</td>
        <td>{{data.email}}</td>
        <td>{{data.Telephone}}</td>
        <td>
        
        </td>
    </tr>
    
  {% endfor %}
  </tbody>

</table>
</div>
{% endblock %}
